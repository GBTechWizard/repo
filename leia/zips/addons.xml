    <extension point="xbmc.addon.repository" name="[COLOR dodgerblue]GBTech[/COLOR] K18 Repository">
        <dir minversion="18.0" maxversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/GBTechWizard/repo/master/leia/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/GBTechWizard/repo/master/leia/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/GBTechWizard/repo/master/leia/zips/</datadir>
        </dir>
        <dir minversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/GBTechWizard/repo/master/matrix/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/GBTechWizard/repo/master/matrix/zips/addons.xml.md5</checksum>
